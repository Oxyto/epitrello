{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		},
		{
			"files": ["*.js", "*.ts", "*.jsx", "*.tsx", "*.html"],
			"options": {
				"plugins": ["prettier-plugin-tailwindcss"],
				"tailwindStylesheet": "./src/app.css"
			}
		}
	]
}
